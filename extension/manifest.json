{
    "name": "Embed Anywhere",
    "version": "0.0.1",
    "manifest_version": 3,
    "description": "Right-click to save highlights to a knowledge base",
    "permissions": [
      "contextMenus",
      "activeTab",
      "scripting",
      "storage"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  